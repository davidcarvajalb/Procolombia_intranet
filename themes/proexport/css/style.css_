/*----------------------------------- Reset -----------------------------------*/

* {  
	margin:0;
	padding:0;
	color: #333333;
}

body{
	background: url("images/bg_main.png") repeat-x 0 -2px #E5E6E7;
	font-family: Arial, Helvetica, sans-serif;
	color: #333;
}

table {
	border-collapse:collapse;
	border-spacing:0;
}

fieldset,img { 
	border:0;
}

address,caption,cite,code,dfn,em,th,var {
	font-style:normal;
	font-weight:normal;
}

ol,ul {
	list-style: none;
}

caption,th {
	text-align:left;
}

h1,h2,h3,h4,h5,h6 {
	font-size:100%;
	font-weight:normal;
	line-height: normal;
}

a{
	text-decoration: none;
	color: #013A81;
}

a:hover {
	text-decoration: underline;
}

.node hr {clear:both;
	background-color: #D1E8FD;
	border: 0 none;
	height: 1px;
	margin: 1.5em 0;
	width:100%;
	float: left;}
p{
	font-size: 12px;
	line-height: 18px;
}

.node .ofice{
	float: left;
	margin: 0 0 1em;
	text-align: center;
}

.node .ofice.izq {
    float: left;
}

.node .ofice.der {
    float: right;
}

.node .ofice.centr {
    margin: 1em auto 1em 4em;
    width: auto;
}
.hr {background-color: #D1E8FD;
border: 0 none;
clear: both;
float: left;
height: 1px;
margin: 1.5em 0;
width: 100%;}
.oportunidades { color:#000000!important;}
.button, .form-submit, #boton-vermas-eventos, #buton-trade{
	background: -moz-linear-gradient(top, #E80519 0%, #C30017 100%); /* firefox */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#E80519), color-stop(100%,#C30017)); /* webkit */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#E80519', endColorstr='#C30017',GradientType=0 ); /* ie */
	display: block;
	border: 0 1px 1px 0 solid #920011;
	line-height: 23px;
	color: #FFF;
	height: 24px;
	font-weight: bold;
	float: left;
	padding: 0 5px;
	border-radius: 3px;
	-moz-border-radius: 3px;
	border: 0 none;
}

.button:hover, .form-submit:hover, #boton-vermas-eventos:hover, #buton-trade:hover{
	background: -moz-linear-gradient(top, #EF2C42 0%, #DF001E 100%); /* firefox */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#EF2C42), color-stop(100%,#DF001E)); /* webkit */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#EF2C42', endColorstr='#DF001E',GradientType=0 ); /* ie */
	cursor: pointer;
	text-decoration: none;
}

/*--------------------------- Estilos generales para un nodo ----------------------------*/

.node h2, .node h3, .node h4, .node h5, .node h6{
